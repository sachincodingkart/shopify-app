               <span class="info-box-icon bg-success elevation-1"><i class="fas fa-shopping-cart"></i></span>

              <div class="info-box-content">
                <span class="info-box-text">TOTAL ITMES</span>
                <span class="info-box-number"> <%= shop_data.product_data.length %></span>
              </div>
              <!-- /.info-box-content -->
            </div>
            <!-- /.info-box -->
          </div>
          <!-- /.col -->

          <!-- fix for small devices only -->
          <div class="clearfix hidden-md-up"></div>

          <div class="col-12 col-sm-6 col-md-6">
            <div class="info-box mb-3">
              <span class="info-box-icon bg-success elevation-1"><i class="fas fa-shopping-cart"></i></span>

              <div class="info-box-content">
                <span class="info-box-text">TOTAL VALUE</span>
                <% total = 0 ; %>
                <% currency = ''; %>
                <% for(var i=0; i < shop_data.product_data.length ; i++) { %>
                   <% remove_currency = shop_data.product_data[i].product_price.split(' '); %>
                   <% currency = remove_currency[0]; %>
                   <% total = +total + +remove_currency[1]; %>
                <% } %>  
                <span class="info-box-number"><%= currency %> <%= total %></span>
              </div>
              <!-- /.info-box-content -->
            </div>
            <!-- /.info-box -->
          
          </div>
          <!-- /.col -->
          <!-- /.col -->
        </div>
        <div class="row">
          <div class="col-lg-6">
            
            <!-- /.card -->

            <div class="card">
              <div class="card-header border-0">
                <h3 class="card-title">CUSTOMERS WISHLIST</h3>
              </div>
              <div class="card-body table-responsive p-0">
                <table class="table table-striped table-valign-middle">
                  <thead>
                  <tr>
                    <th>CUSTOMER</th>
                    <th>PRICE</th>
                    <th>TIME</th>
                  </tr>
                  </thead>
                  <tbody>
                   <% for(var i=0; i < shop_data.user_data.length ; i++) { %>  
                  <tr>
                    <td>
                      <%= shop_data.user_data[i].customer_name %>
                    </td>

                    <td>
                      <% cust_total = 0 ; 
                          for(var j=0; j < shop_data.product_data.length ; j++) { 
                           if( shop_data.product_data[j].customer_id ==  shop_data.user_data[i].customer_id) {
                              remove_currency = shop_data.product_data[j].product_price.split(' '); 
                              cust_total = +cust_total + +remove_currency[1]; 
                            }
                          } %> 
                          <%= currency %> <%= cust_total %> 
                    </td>

                    <td>
                      <%
                       var pro_time = shop_data.product_data[i].product_time.split('T'); 
                       var date1 = pro_time[0];
                          var date2 = dateFormat(new Date(), "mm/dd/yy, h:MM:ss TT");
                          var diffTime = Math.abs(date2 - date1);
                          var diffDays = Math.ceil(diffTime / (1000 * 60 * 60 * 24)); 
                       %> 
                        <%= date1 +'hello' %> <%= date2 +'ssssssssss'%> <%= shop_data.current_time %> 
                    </td>
                  </tr>
                   <% } %>  
                  </tbody>
                </table>
              </div>
            </div>
            <!-- /.card -->
          </div>
          <!-- /.col-md-6 -->
          <div class="col-lg-6">
            
            <!-- /.card -->

