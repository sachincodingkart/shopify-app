                    <th>TOP PRODUCTS</th>
                    <th>PRICE</th>
                    <th>IN WISHLISTS</th>
                  </tr>
                  </thead>
                  <tbody>
                 <% for(var i=0; i < shop_data.product_data.length ; i++) { %>                      
                  <tr>

                    <td>
                     <%= shop_data.product_data[i].product_title %>
                    </td>

                    <td>
                      <%= shop_data.product_data[i].product_price %>
                    </td>
                    
                    <td>
                      1
                    </td>
                  </tr>
                 <% } %>  
                  </tbody>
                </table>
              </div>
