              </div>
              <div class="card-body table-responsive p-0">
                <table class="table table-striped table-valign-middle">
                  <thead>
                  <tr>
                    <th>CUSTOMER</th>
                    <th>PRICE</th>
                    <th>TIME</th>
                  </tr>
                  </thead>
                  <tbody>
                   <% for(var i=0; i < shop_data.user_data.length ; i++) { %>  
                  <tr>
                    <td>
                      <%= shop_data.user_data[i].customer_name %>
                    </td>

                    <td>
                      <% cust_total = 0 ; 
                          for(var j=0; j < shop_data.product_data.length ; j++) { 
                           if( shop_data.product_data[j].customer_id ==  shop_data.user_data[i].customer_id) {
                              remove_currency = shop_data.product_data[j].product_price.split(' '); 
                              cust_total = +cust_total + +remove_currency[1]; 
                            }
                          } %> 
                          <%= currency %> <%= cust_total %> 
                    </td>

                    <td>
                      <%
                       var date_obj = new Date().toISOString().split('T');
                       var date1 = date_obj[0];
                       var pro_time = shop_data.product_data[i].product_time.split('T'); 
                       var date2 = pro_time[0];
                       var date3 = (Date.parse(date1) - Date.parse(date2)) / 86400000;
                       
                       var time1 = date_obj[1].split('.');
                       var time2 = pro_time[1].split('.');
                       var diff  = ( new Date(date1+' ' +  time1[0] ) - new Date(date1+' '+ time2[0] ))/1000/60/60;
                       var hour = Math.floor(diff);
                       %> 
                       <% if(date3 == 0) { %>
                           <% if(hour == 0) { %>
                              <%= 'few minute ago' %>
                           <% } else { %>
                             <%=  hour +' hour ago' %>
                           <% } %> 
                       <% } else { %>
                        <%= date3+' days ago' %>
                       <% } %> 
                       <!-- <%=  diff  %> -->
                    </td>
                  </tr>
                   <% } %>  
                  </tbody>
                </table>
              </div>
            </div>
            <!-- /.card -->
          </div>
